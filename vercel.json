{
    "version": 2,
    "builds": [
      {
        "src": "group_web/wsgi.py",
        "use": "@vercel/python"
      },
      {
        "src": "build_files.sh",
        "use": "@vercel/static-build",
        "config": {
          "distDir": "staticfiles_build"
        }
      },
      {
        "src": "manage.py",
        "use": "@vercel/python",
        "config": { "zeroConfig": true }
      },
      {
        "src": "staticfiles_build/statics/**/*",
        "use": "@vercel/static"
      }
    ],
    "routes": [
      {
        "src": "/static/(.*)",
        "dest": "/static/$1"
      },
      {
        "src": "/(.*)",
        "dest": "group_web/wsgi.py"
      }
    ]
  }